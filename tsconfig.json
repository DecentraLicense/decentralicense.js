{
  "compilerOptions": {
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": [
      "es2016"
    ],
    "target": "es5",
    "module": "commonjs",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true
  }
}